# Tag replace regex:
#   <([^<]+)>
#   &lt;\1&gt;  
# Tag highlight regex:
#   (&lt;[^&]+&gt;)
#   <span class="tag">\1</span> 
# String replace regex:
#   (\"[^"]+\")&
#   <span class="string">\1</span>&
def sum_up_to(num)
  return 0 if num == 0
  return num + sum_up_to(num-1)
end
puts (sum_up_to 10)

puts :>.class
